      "cell_type": "code",
      "source": [
        "# Mauricio (02/04/2026)\n",
        "\n",
        "import sqlite3\n",
        "import pandas as pd\n",
        "\n",
        "# Explicitly define path_sqlite_local to ensure it's accessible\n",
        "path_sqlite_local = '/content/nba_descomprimido/nba.sqlite'\n",
        "\n",
        "# 1. abrimos una conexion\n",
        "conexion = sqlite3.connect(path_sqlite_local)\n",
        "\n",
        "# 2. Definimos las tablas que t√∫ necesitas para el Diagrama Estrella\n",
        "tablas_interes = ['common_player_info', 'game', 'draft_combine_stats']\n",
        "\n",
